

/************************** Function Prototypes ******************************/
extern int CanPs_Polled_init(u16 DeviceId, XCanPs *CanInstPtr);
extern int SendFrame(XCanPs *InstancePtr);
extern int RecvFrame(XCanPs *InstancePtr);


#define RATE_BUF_SIZE 6
typedef struct{
	Xint32 raw_value;
	Xint32 last_raw_value;
	Xint32 ecd_value;
	Xint32 diff;
	Xint32 temp_count;
	Xint32 buf_count;
	Xint32 ecd_bias;
	Xint32 ecd_raw_rate;
	Xint32 rate_buf[RATE_BUF_SIZE];
	Xint32 round_cnt;
	Xint32 filter_rate;
	float ecd_angle;
} Encoder;




